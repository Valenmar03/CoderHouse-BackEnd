<a href="/profile"><button class="returnHome-btn" style="color: black; width: 10%;">Volver</button></a>

<h1>Administracion de Usuarios</h1>

<div class="container">
    <div class="cards-container">
        {{#each user}}
            <div class="users-card">
                <p>{{this.first_name}} {{this.last_name}}</p>
                <p>Rol: {{this.role}}</p>
                <div class="btns">
                    <button class="role">Modificar rol</button>
                    <button class="delete">Eliminar</button>
                </div>
            </div>
        {{/each}}
    </div>
</div>